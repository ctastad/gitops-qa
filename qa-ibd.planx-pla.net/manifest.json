{
  "notes": [
    "This is the qa environment manifest.",
    "That's all I have to say"
  ],
  "versions": {
    "arborist": "quay.io/cdis/arborist:integration202108",
    "aws-es-proxy": "quay.io/cdis/aws-es-proxy:0.8",
    "dashboard": "quay.io/cdis/gen3-statics:integration202108",
    "fence": "quay.io/cdis/fence:integration202108",
    "indexd": "quay.io/cdis/indexd:integration202108",
    "peregrine": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/peregrine:3.1.0",
    "pidgin": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/pidgin:1.2.1",
    "revproxy": "quay.io/cdis/nginx:integration202108",
    "sheepdog": "quay.io/cdis/sheepdog:integration202108",
    "portal": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/data-portal:3.5.0",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "spark": "quay.io/cdis/gen3-spark:integration202108",
    "tube": "quay.io/cdis/tube:integration202108",
    "hatchery": "quay.io/cdis/hatchery:integration202108",
    "wts": "quay.io/cdis/workspace-token-service:integration202108",
    "ambassador": "quay.io/datawire/ambassador:1.4.2",
    "manifestservice": "quay.io/cdis/manifestservice:integration202108",
    "guppy": "quay.io/cdis/guppy:integration202108",
    "ssjdispatcher": "quay.io/cdis/ssjdispatcher:integration202108",
    "metadata": "quay.io/cdis/metadata-service:integration202108",
    "sower": "quay.io/cdis/sower:integration202108"
  },
  "indexd": {
    "arborist": "true"
  },
  "sower": [
    {
      "name": "ingest-metadata-manifest",
      "action": "ingest-metadata-manifest",
      "activeDeadlineSeconds": 86400,
      "serviceAccountName": "jobs-qa-ibd-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/metadata-manifest-ingestion:integration202108",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "manifest-indexing",
      "action": "index-object-manifest",
      "serviceAccountName": "jobs-qa-ibd-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/manifest-indexing:integration202108",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "indexd-manifest",
      "action": "download-indexd-manifest",
      "serviceAccountName": "jobs-qa-ibd-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/download-indexd-manifest:integration202108",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    }
  ],
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:integration202108"
    }
  },
  "global": {
    "environment": "qaplanetv2",
    "hostname": "qa-ibd.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:707767160287:certificate/c676c81c-9546-4e9a-9a72-725dd3912bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/ibdgc-dictionary/1.6.5/schema.json",
    "portal_app": "gitops",
    "kube_bucket": "kube-qaplanetv2-gen3",
    "logs_bucket": "logs-qaplanetv2-gen3",
    "sync_from_dbgap": "False",
    "dispatcher_job_num": "10",
    "netpolicy": "on",
    "useryaml_s3path": "s3://cdis-gen3-users/qa/user.yaml",
    "maintenance_mode": "off"
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "qa-ibd",
        "type": "participant"
      },
      {
        "index": "qa-ibd_file",
        "type": "file"
      },
      {
        "index": "qa-ibd_supplementary_file",
        "type": "supplementary_file"
      }
    ],
    "config_index": "qa-ibd_array-config",
    "auth_filter_field": "auth_resource_path"
  },
  "arborist": {
    "deployment_version": "2"
  }
}
